{
  "name": "app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --tsc",
    "start:debug": "nest start --debug 0.0.0.0:9229 --tsc --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:start": "npx prisma db seed",
    "migration:dev": "npx prisma migrate dev --create-only --preview-feature",
    "migration:dev-dploy": "npx prisma migrate deploy --preview-feature",
    "migration:dump": "npx prisma db push && npx prisma generate",
    "migration:dev-generate": "npx prisma generate",
    "migration:start": "npx prisma migrate dev --preview-feature"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/throttler": "^6.1.1",
    "@prisma/client": "5.15.1",
    "@tirke/node-cache-manager-ioredis": "^3.6.0",
    "@types/cron": "^2.4.0",
    "@types/lodash": "^4.17.4",
    "aws-sdk": "^2.1640.0",
    "aws-sdk-mock": "^6.0.4",
    "axios": "^1.7.2",
    "cache-manager": "^5.6.1",
    "cache-manager-redis": "^0.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "ioredis": "^5.4.1",
    "jest-mock-extended": "^3.0.7",
    "lodash": "^4.17.21",
    "mathjs": "^12.4.2",
    "mime-types": "^2.1.35",
    "nest-winston": "^1.10.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "ts-jenum": "^2.2.2",
    "types": "^0.1.1",
    "uuid": "^10.0.0",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-sonarjs": "^1.0.3",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "5.15.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
